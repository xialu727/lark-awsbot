# AWS配置
AWS_REGION=us-east-1
AWS_ACCOUNT=123456789012
DYNAMODB_TABLE=aws-tickets

# 飞书应用配置
FEISHU_APP_ID=your_feishu_app_id_here
FEISHU_APP_SECRET=your_feishu_app_secret_here
FEISHU_ENCRYPT_KEY=your_encrypt_key_here
FEISHU_VERIFICATION_TOKEN=your_verification_token_here

# 飞书API配置
FEISHU_API_BASE_URL=https://open.feishu.cn/open-apis
FEISHU_TOKEN_URL=https://open.feishu.cn/open-apis/auth/v3/tenant_access_token/internal

# AWS Support配置
AWS_SUPPORT_REGION=us-east-1
AWS_SUPPORT_LANGUAGE=zh
AWS_SUPPORT_SEVERITY=low

# 应用配置
DEBUG=false
LOG_LEVEL=INFO
MAX_HISTORY_RECORDS=10
TICKET_TIMEOUT=3600
RETRY_MAX_ATTEMPTS=3
RETRY_DELAY=1

# Lambda配置
LAMBDA_TIMEOUT=300
LAMBDA_MEMORY=512

# 群聊配置
CHAT_NAME_PREFIX=AWS工单-
CHAT_DESCRIPTION=AWS技术支持工单讨论群
MAX_CHAT_MEMBERS=50

# 监控配置
ENABLE_METRICS=true
METRICS_NAMESPACE=FeishuBot
ALARM_EMAIL=your_alarm_email@example.com